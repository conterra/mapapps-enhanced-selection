    {
      "name": "SelectFeatureFromLayerController",
      "provides": [
        "ct.framework.api.EventHandler"
      ],
      "properties": {
        "Event-Topics": [
          {
            "topic": "ct/dn_enhancedselection/DRAW_GEOMETRY",
            "method": "draw"
          },
          {
            "topic": "ct/dn_enhancedselection/GEOMETRY_DRAWN",
            "method": "geometryDrawn"
          }
