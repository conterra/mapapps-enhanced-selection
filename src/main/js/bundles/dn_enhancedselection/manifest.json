        },
        {
          "name": "drawGeometryHandler",
          "providing": "dn_enhancedselection.DrawGeometryHandler"
        },
        {
          "name": "polygonWidget",
          "providing": "dn_enhancedselection.PolygonWidget",
          "cardinality": "0..1"
        },
        {
          "name": "_mapState",
