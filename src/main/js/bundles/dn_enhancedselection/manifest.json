      },
      "provides": [
        "dijit.Widget",
        "dn_enhancedselection.GeometryInputProvider",
        "dn_enhancedselection.FreehandPolygonWidget"
      ],
      "references": [
        {
          "name": "_eventService",
          "providing": "ct.framework.api.EventService"
        }
      ]
    },
    {
      "name": "PolygonWidget",
      "propertiesConstructor": true,
      "properties": {
        "Service-Ranking": -3
      },
      "provides": [
        "dijit.Widget",
        "dn_enhancedselection.GeometryInputProvider",
        "dn_enhancedselection.PolygonWidget"
      ],
      "references": [
        {
